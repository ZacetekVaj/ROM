Cell[BoxData[{
 RowBox[{"ClearAll", "[", 
  RowBox[{"x", ",", " ", "y"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"EnacbaNosilke", "[", 
   RowBox[{"Daljica", "[", 
    RowBox[{"AA_", ",", "BB_"}], "]"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x1", ",", " ", "y1", ",", " ", "x2", ",", " ", "y2", ",", " ", "k", ",",
       " ", "n"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x1", ",", " ", "y1"}], "}"}], "=", "AA"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x2", ",", " ", "y2"}], "}"}], "=", "BB"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"k", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"y2", " ", "-", " ", "y1"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"x2", " ", "-", " ", "x1"}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", 
      RowBox[{"n", " ", "/.", 
       RowBox[{"First", "[", 
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{"y1", " ", "==", " ", 
           RowBox[{
            RowBox[{"k", "*", "x1"}], " ", "+", " ", "n"}]}], ",", " ", "n"}],
          "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"y", " ", "==", "  ", 
      RowBox[{
       RowBox[{"k", "*", "x"}], " ", "+", " ", "n"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellChangeTimes->{{3.750508118042511*^9, 3.750508137148896*^9}, {
